---
import {
  fetchContentByType,
  convertMarkdownToHTML,
} from '../../services/microcms'
import SectionWrapper from '../shared/SectionWrapper.astro'

const { content } = await fetchContentByType('links')
const parsedContent = await convertMarkdownToHTML(content)
---

<SectionWrapper heading="外部リンク">
  <Fragment set:html={parsedContent} />
</SectionWrapper>
